<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="AnnexDetailedVersionRow" parent="GtkListBoxRow">
    <property name="activatable">0</property>
    <property name="selectable">0</property>
    <child>
      <object class="GtkBox">
        <property name="margin-start">12</property>
        <property name="margin-end">12</property>
        <property name="margin-top">8</property>
        <property name="margin-bottom">8</property>
        <property name="spacing">12</property>
        <child>
          <object class="GtkLabel" id="versionLabel">
            <property name="hexpand">1</property>
            <property name="vexpand">1</property>
            <property name="valign">center</property>
            <property name="xalign">0</property>
            <attributes>
              <attribute name="weight" value="bold"/>
            </attributes>
          </object>
        </child>
        <child>
          <object class="GtkLabel" id="shellVersionLabel">
            <property name="vexpand">1</property>
            <property name="valign">center</property>
            <property name="xalign">0</property>
            <style>
              <class name="dim-label"/>
            </style>
          </object>
        </child>
        <child>
          <object class="GtkButton" id="downloadButton">
            <property name="action-name">version.download</property>
            <property name="icon-name">document-save-symbolic</property>
            <property name="tooltip-text" translatable="yes">Download</property>
            <property name="valign">center</property>
            <style>
              <class name="circular"/>
            </style>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>

